if game.PlaceId == 3102144307 then
	local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
	local Window = Library.CreateLib("Anime Clicker Simulator gui by LagunScripts", "Sentinel")

	-----AutoFarm
	
	local Tab = Window:NewTab("AutoFarm")
	local Section = Tab:NewSection("AutoClick")
	Section:NewToggle("AutoClick", "It will automacly click", function(state)
		if state then
			getgenv().sdss = true;

			while wait() do
				if getgenv().sdss == true then
					game:GetService("ReplicatedStorage").Remotes.ClickRemote:FireServer(false, "Clicker!", nil)
				end
			end

		else
			getgenv().sdss = false;

			while wait() do
				if getgenv().sdss == true then
					game:GetService("ReplicatedStorage").Remotes.ClickRemote:FireServer(false, "Clicker!", nil)
				end
			end

		end
	end)
	
