	----Misc
	
	local Tab = Window:NewTab("Misc")
	local Section = Tab:NewSection("Misc")
	Section:NewButton("Anti-Afk", "Makes that you cant get kicked from game for afk", function()
		local vu = game:GetService("VirtualUser")
		game:GetService("Players").LocalPlayer.Idled:connect(function()
			vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
			wait(1)
			vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
		end)
	end)
	Section:NewToggle("AutoDailyRewards", "Automacly claims daily rewards", function(state)
		if state then
			getgenv().Rew1 = true;

			while wait() do
				if getgenv().Rew1 == true then
					game:GetService("ReplicatedStorage").Remotes.CollectChest:InvokeServer("DailyRewards")
				end
			end
		else
			getgenv().Rew1 = false;

			while wait() do
				if getgenv().Rew1 == true then
					game:GetService("ReplicatedStorage").Remotes.CollectChest:InvokeServer("DailyRewards")
				end
			end
		end
	end)
	Section:NewToggle("AutoGroupRewards", "Automacly collects group rewards", function(state)
		if state then
			getgenv().Rew2 = true;

			while wait() do
				if getgenv().Rew2 == true then
					game:GetService("ReplicatedStorage").Remotes.CollectChest:InvokeServer("GroupRewards")
				end
			end

		else
			getgenv().Rew2 = false;

			while wait() do
				if getgenv().Rew2 == true then
					game:GetService("ReplicatedStorage").Remotes.CollectChest:InvokeServer("GroupRewards")
				end
			end

		end
	end)
